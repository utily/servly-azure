{
  "name": "servly-azure",
  "version": "0.1.13",
  "description": "Azure Functions target for Servly.",
  "author": "PayFunc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/payfunc/servly-azure"
  },
  "bugs": {
    "url": "https://github.com/payfunc/servly-azure/issues"
  },
  "homepage": "https://payfunc.com/open-source/servly-azure",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "git": {
    "tagName": "v${version}"
  },
  "github": {
    "release": true
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    },
    "testEnvironment": "node",
    "testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "node_modules/",
      "dist/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "preset": "ts-jest",
    "testMatch": null
  },
  "scripts": {
    "dev": "watch 'npm run build' .",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "fix": "eslint '**/*.{js,ts,tsx}' --fix",
    "build": "tsc -p .",
    "test": "jest",
    "test:watch": "watch jest",
    "prepare": "npm run build",
    "clean": "rm -rf dist node_modules coverage"
  },
  "dependencies": {
    "@azure/functions": "^1.2.2",
    "isoly": "0.0.35",
    "servly": "0.1.15"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "babel-jest": "^26.0.1",
    "eslint": "8.4.1",
    "eslint-plugin-prettierx": "github:utily/eslint-plugin-prettierx#utily-20231004",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26",
    "prettierx": "github:utily/prettierx#utily-20231004",
    "ts-jest": "^26.1.0",
    "typescript": "4.4.3",
    "watch": "0.13.0"
  }
}
